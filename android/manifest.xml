<!--START_PLUGINS_MANIFEST-->
<!-- GameThrive -->
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.WAKE_LOCK" />
<uses-permission android:name="android.permission.VIBRATE" />
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
<uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
<uses-permission android:name="android.permission.GET_ACCOUNTS" />

<permission android:name="com.hashcube.sudokuquest.permission.C2D_MESSAGE" android:protectionLevel="signature" />
<uses-permission android:name="com.hashcube.sudokuquest.permission.C2D_MESSAGE" />

<uses-permission android:name="com.amazon.device.messaging.permission.RECEIVE" />
<permission android:name="com.hashcube.sudokuquest.permission.RECEIVE_ADM_MESSAGE" android:protectionLevel="signature" />
<uses-permission android:name="com.hashcube.sudokuquest.permission.RECEIVE_ADM_MESSAGE" />
<!--END_PLUGINS_MANIFEST-->
<!--START_PLUGINS_APPLICATION-->
<!-- GameThrive -->
<meta-data android:name="googleProjectNo" android:value="" />
<meta-data android:name="gameThriveAppID" android:value="" />
<!-- Permissions -->
<meta-data android:name="com.google.android.gms.version"
           android:value="@integer/google_play_services_version" />
<activity android:name="com.gamethrive.NotificationOpenedActivity"/>
<receiver
    android:name="com.gamethrive.GcmBroadcastReceiver"
    android:permission="com.google.android.c2dm.permission.SEND" >
  <intent-filter>
    <action android:name="com.google.android.c2dm.intent.RECEIVE" />
    <category android:name="com.hashcube.sudokuquest" />
  </intent-filter>
</receiver>
<receiver
   android:name="com.tealeaf.plugin.plugins.GameThrivePlugin"
   android:exported="false">
  <intent-filter>
     <action android:name="com.gamethrive.BackgroundBroadcast.RECEIVE" />
  </intent-filter>
</receiver>
<service android:name="com.gamethrive.GcmIntentService" />

<amazon:enable-feature android:name="com.amazon.device.messaging" android:required="false"/>
<activity android:name="com.gamethrive.NotificationOpenedActivity"/>
<service android:name="com.gamethrive.ADMMessageHandler" android:exported="false" />
<receiver
    android:name="com.gamethrive.ADMMessageHandler$Receiver"
    android:permission="com.amazon.device.messaging.permission.SEND" >
  <intent-filter>
    <action android:name="com.amazon.device.messaging.intent.REGISTRATION" />
    <action android:name="com.amazon.device.messaging.intent.RECEIVE" />
    <category android:name="com.hashcube.sudokuquest" />
  </intent-filter>
</receiver>
<!--END_PLUGINS_APPLICATION-->
